{
  "presets": ["next/babel"],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "@": "./",
          "lib/payload": "./lib/payload",
          "components/PayloadRichText": "./components/PayloadRichText",
          "'lib/payload'": "./lib/payload",
          "'components/PayloadRichText'": "./components/PayloadRichText"
        },
        "extensions": [".js", ".jsx", ".ts", ".tsx"]
      }
    ]
  ]
}
